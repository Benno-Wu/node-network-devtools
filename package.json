{
  "name": "node-network-devtools",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "start": "turbo start",
    "ts-start": "turbo ts-start",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev:main": "turbo dev --filter=node-network-devtools",
